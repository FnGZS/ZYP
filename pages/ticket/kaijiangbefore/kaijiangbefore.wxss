page {
    background: #f0f0f0;
}

.padding-bottom-118 {
    padding-bottom: 118rpx;
}

.box1 {
    border: 1rpx solid #c0c0c0;
    margin: 30rpx;
    border-radius: 10rpx;
    background: #fff;
}

.box1-list {
    height: 90rpx;
    line-height: 90rpx;
    display: flex;
    justify-content: space-between;
    margin: 0 30rpx;
    border-bottom: 1rpx solid #ebebeb;
    font-size: 32rpx;
    align-items: center;
}

.color_red {
    color: #cc6054;
}

.box1-list image {
    width: 60rpx;
    height: 60rpx;
}

.box1-list1 {
    display: flex;
    padding: 20rpx 30rpx;
}

button::after {
    border: navajowhite;
}

.btn {
    height: 80rpx;
    line-height: 80rpx;
    color: #fff;
    font-size: 32rpx;
    border-radius: 70rpx;
    background: #e15f4f;
    border: 1rpx solid #a9493d;
    flex: 1 1 auto;
    font-weight: bold;
}

.box2 {
    margin: 20rpx 30rpx;
    background: #fff6e5;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border: 1rpx solid #c2b69e;
    border-radius: 10rpx;
    color: #856733;
    font-size: 30rpx;
    position: relative;
}

.box2::after {
    content: "";
    height: 10rpx;
    width: 10rpx;
    border: 1rpx solid #c3b79f;
    background: #f0e0c6;
    border-radius: 50%;
    position: absolute;
    top: 10rpx;
    left: 10rpx;
}

.box2::before {
    content: "";
    height: 10rpx;
    width: 10rpx;
    border: 1rpx solid #c3b79f;
    background: #f0e0c6;
    border-radius: 50%;
    position: absolute;
    bottom: 10rpx;
    left: 10rpx;
}

.box2-con {
    position: relative;
    font-weight: bold;
}

.box2-con::after {
    content: "";
    height: 10rpx;
    width: 10rpx;
    border: 1rpx solid #c3b79f;
    background: #f0e0c6;
    border-radius: 50%;
    position: absolute;
    top: 10rpx;
    right: 10rpx;
}

.box2-con::before {
    content: "";
    height: 10rpx;
    width: 10rpx;
    border: 1rpx solid #c3b79f;
    background: #f0e0c6;
    border-radius: 50%;
    position: absolute;
    bottom: 10rpx;
    right: 10rpx;
}

.fix-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 118rpx;
    border-top: 1rpx solid #c6c6c6;
    padding: 20rpx;
    box-sizing: border-box;
}

.fix-bottom input {
    height: 100%;
    line-height: 100%;
    width: 100%;
    border: 1rpx solid #c6c6c6;
    border-radius: 10rpx;
    font-size: 30rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    background: #fff;
}

.danmu {
    position: fixed;
    z-index: -1;
    height: 100%;
    width: 100%;
    padding-top: 380rpx;
    top: 0;
    left: 0;
    padding-bottom: 118rpx;
    box-sizing: border-box;
}

.danmu-con {
    height: 100%;
    position: relative;
    width: 100%;
}

