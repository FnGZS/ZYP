<view class="container">
    <view class="con-title font-26 color_808080">
    发布的
  </view>
    <view bindtap="goTicketresults" class="list bg_fff" data-gid="{{item.gid}}" data-oid="{{item.oid}}" style="border-top:none" >
        <view class="list-body">
            <view class="list-txt">
                <view class="list-tile flex-1-1 font-32  list-2" >奖品：aa</view>
                <view class="font-32 flex-0-0 line-height-47">X1{{item.count}}</view>
                <view class="font-26 color_808080 flex-0-0 margin-left-20">a{{item.endtime}}</view>
                <text class="iconfont icon-you line-h-40 font-26 flex-0-0 color_808080"></text>
            </view>
        </view>
    </view>
</view>