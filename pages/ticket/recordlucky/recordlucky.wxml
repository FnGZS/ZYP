<view class="container">
    <view class="con-title font-26 color_808080">
    已中奖
  </view>
    <view bindtap="goluckyDetail" class="list bg_fff" data-gid="{{item.gid}}"  style="border-top:none" wx:for='{{luckyList}}' wx:key="*this">
        <view class="list-body">
            <view class="list-txt">
                <view class="list-tile flex-1-1 font-32  list-2"  >奖品：{{item.items[0].luckPrize}} X {{item.items[0].num}}</view>
                <view class="font-32 flex-0-0 line-height-47"></view>
                <view class="font-26 color_808080 flex-0-0 margin-left-20" >{{item.lotteryTime}}</view>
                <text class="iconfont icon-you line-h-40 font-26 flex-0-0 color_808080"></text>
            </view>
        </view>
    </view>
</view>