#secondHand{
  width: 100%;
}
.search{
  width: 100%;
  height: 50rpx;
  background: #ef696c;
  padding-top: 15rpx;
  padding-bottom:15rpx;
  position: fixed; 
  z-index: 999;
}
.search-main{
  margin-left: 3%;
  height: 100%;
  width: 94%;
  background: #fff;
  border-radius: 25rpx;
  display: flex;
  justify-content: center;
}
.icon-sousuo{
  font-size: 30rpx;
  color: #CACACA;
}
.search-main-text{
  font-size: 25rpx;
  line-height: 48rpx;
  margin-left: 5rpx;
  color: #CACACA;
}
.secondHand-swiper{
  padding-top:80rpx;
}
.secondHand-head-swiper-image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.line{
  width: 100%;
  height: 10rpx;
  background: #D3D3D3;
}
.tab {
  width: 100%;
  /* margin-top: 10rpx; */
  top: 0;
  left: 0;
  z-index: 100;
  white-space: nowrap;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 70rpx;
  background: white;
  border-top: 10rpx solid #f5f5f5;
  border-bottom: 2rpx solid #f5f5f5;
  padding-left: 3%;
  padding-right: 3%;
}

.tab-item {
  display: inline-block;
  text-align: center;
  font-size: 30rpx;
  padding-left: 25rpx;
  padding-right: 25rpx;
}

.active {
  border-bottom: 5rpx solid #ef696c;
  color: #ef696c;
}

.active1 {
  background: #ef696c;
}
.stickyClass{
    position: sticky;
    top: 70rpx;
}
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
.goods-list-main{
  margin-left: 3%;
  width: 94%;
  height: 220rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  display: flex;
  border-bottom: 1px solid #F6F6F6;
}
.goods-list-main-img{
  width: 30%;
  height: 100%;
  border-radius: 10rpx;
}
.goods-list-mes{
  width: 70%;
  position: relative;
}
.goods-list-mes-title{
  position: absolute;
  font-size: 28rpx;
  margin-left: 3%;
  top: 0;
  left: 0;
  color: #585858;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.goods-list-mes-price{
  position: absolute;
  bottom: 40rpx;
  left: 0;
  margin-left: 3%;
  color: #ef696c;
  display: flex;
}
.goods-list-mes-price-flag{
  font-size: 20rpx;
  margin-top: 10rpx;
}
.goods-list-mes-price-num{
  font-size: 28rpx;
  margin-left: 5rpx;
}
.goods-list-mes-pos{
  position: absolute;
  font-size: 23rpx;
  bottom: 10rpx;
  left: 0;
  margin-left: 3%;
  width: 60%;
  color: #CCCCCC;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.icon-pos{
  font-size: 23rpx;
  line-height: 30rpx;
  padding-top: 20rpx;
}
.goods-list-mes-goodsWay{
  position: absolute;
  font-size: 23rpx;
  right: 0;
  bottom: 40rpx;
  color: #FF3434;
}
.goods-list-mes-browse{
  position: absolute;
  font-size: 23rpx;
  right: 0;
  bottom: 7rpx;
  color: #CCCCCC;
}
.fabu {
  position: fixed;
  right: 50rpx;
  border-radius: 50%;
  bottom: 50rpx;
  width: 100rpx;
  height: 100rpx;
  background: #ef696c;
  text-align: center;
  /* opacity: 0.9; */
}

.icon-fabu {
  color: white;
  font-size: 45rpx;
  line-height: 100rpx;
}
