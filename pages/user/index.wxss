#my {
  width: 100%;
  height: 100%;
}

.userInfo {
  width: 100%;
  height: 350rpx;
  background: linear-gradient(to bottom, #ef696c, #ffa07a);
  position: relative;
}

.userinfo-avatar {
  position: absolute;
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  border: 5rpx solid white;
  left: 50%;
  margin-left: -64rpx;
  margin-top: 20rpx;
}

.userinfo-name {
  position: absolute;
  width: 100%;
  text-align: center;
  top: 160rpx;
  color: #fff;
  font-size: 30rpx;
}

.userinfo-balance {
  position: absolute;
  width: 100%;
  text-align: center;
  top: 200rpx;
  color: #fff;
  font-size: 25rpx;
}

.order {
  position: relative;
  width: 94%;
  height: 210rpx;
  background: #fff;
  border-radius: 20rpx;
  margin-left: 3%;
  margin-top: -105rpx;
  box-shadow: 5rpx 5rpx 5rpx #a9a9a9;

}

.order-title {
  width: 100%;
  height: 80rpx;
  position: relative;
  border-bottom: 1px solid #f5f5f5;
}

.order-title-text {
  font-size: 28rpx;
  font-weight: bold;
  line-height: 80rpx;
  margin-left: 5%;
}

.order-title-btn {
  position: absolute;
  right: 5%;
  top: 0;
  font-size: 23rpx;
  line-height: 80rpx;
  color: #ef696c;
}

.order-main {
  display: flex;
