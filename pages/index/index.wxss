page {
  width: 100%;
  height: 100%;
   background-color:white;
}

.guide-all {
  width: 100%;
  height: 100%;
  background: white;
  position: absolute;
  left: 0;
  top: 0;
 
  /* background: pink; */
  animation: Gradual 12s forwards;
}

@keyframes Gradual {
  0% {
    opacity: 0.0;
  }

  25% {
    opacity: 1;
  }

  50% {
    opacity: 1;
  }

  65% {
    opacity: 1;
  }

  90% {
    opacity: 0.0;
  }
  100%{
    opacity: 0.0;
  }
}

.guide {
  width: 100%;
  height: 80%;
  bottom: 0rpx;
  /* background: rebeccapurple; */
  position: fixed;
}

.guide-top {
  width: 100%;
  height: 300rpx;
  /* background: antiquewhite; */
  display: flex;
  align-items: center;
  justify-content: center;
}

.guide-top-img {
  width: 250rpx;
  height: 250rpx;
  animation: rotate 12s;
  /* background: rosybrown; */
}

@keyframes rotate {
  0% {
    width: 100rpx;
    height: 100rpx;
    /* margin-left: 0rpx; */
    opacity: 0.0;
  }

  25% {
    width: 250rpx;
    height: 250rpx;
    margin-top:0rpx;
    margin-left: 0rpx;
    opacity: 1;
    transform: rotateY(1440deg);
  }
  50%{
  width: 250rpx;
    height: 250rpx;
        margin-top:0rpx;
        /* margin-left: 0rpx; */
    opacity: 1;
       transform: rotateY(1440deg);
  }
  70%{
    width: 100rpx;
    height: 100rpx;
        margin-top:-50rpx;
        /* margin-left: -10rpx; */
    opacity: 1;
       transform: rotateY(1440deg);
  }
  100%{
        margin-top:-50rpx;
        /* margin-left: -10rpx; */
     width: 100rpx;
    height: 100rpx;
    opacity: 1;
       transform: rotateY(1440deg);
  }
}

.guide-middle {
  width: 100%;
  height: 200rpx;
  /* background: coral; */
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.guide-middle-left {
  width: 250rpx;
  height: 80rpx;
  opacity: 0.0;
  /* background: aliceblue; */
  animation: guideleft 12s linear ;
}

.guide-middle-rigth {
  width: 250rpx;
  height: 80rpx;
  opacity: 0.0;
  /* background: aliceblue; */
  animation: guiderigth 12s linear ;
}

@keyframes guideleft {
  0% {
    opacity: 0.0;
  }

  25% {
    opacity: 0.0;
  }

  50% {
    opacity: 1;
    margin-top: 0rpx;
    margin-right: 0rpx;
    width: 250rpx;
    height: 80rpx;
  }

  75% {
    opacity: 1;
    width: 180rpx;
    height: 60rpx;
    margin-top: -480rpx;
    margin-right: -120rpx;
  }

  100% {
    opacity: 0.0;
    width: 180rpx;
    height: 60rpx;
     margin-top: -480rpx;
    margin-rigth: -120rpx;
  }
}

@keyframes guiderigth {
  0% {
    opacity: 0.0;
  }

  25% {
    opacity: 0.0;
  }

  50% {
    opacity: 1;
    width: 250rpx;
    height: 80rpx;
    margin-top: 0rpx;
    margin-left: 0rpx;
  }

  75% {
    opacity: 1;
    width: 180rpx;
    height: 60rpx;
    margin-top: -480rpx;
    margin-left: -120rpx;
  }

  100% {
    opacity: 0;
    width: 180rpx;
    height: 60rpx;
     margin-top: -480rpx;
    margin-left: -120rpx;
  }
}

.guide-top-img image {
  width: 100%;
  height: 100%;
}

.guide-middle-image {
  width: 100%;
  height: 100%;
}

.index-head, .index-head-swiper {
  width: 750rpx;
  height: 400rpx;
}

.index-head {
  position: relative;
 
}

.index-head-bg {
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 1;
  bottom: 0;
  width: 100%;
  height: 93rpx;
}

.index-head-bg image {
  width: 94%;
  height: 100%;
}

.index-head swiper {
  width: 100%;
  display: block;
  height: 100%;
}

.index-head-swiper-image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.conter {
  width: 100%;
  height: 100%;
  animation: opaque 12s forwards;
}
.conter-no {
  width: 100%;
  height: 100%;
    animation: opaque-no 2s forwards;
}
@keyframes opaque-no{
  0%{
    opacity: .0;
  }
  100%{
    opacity: 1；
  }
}
@keyframes opaque {
  0% {
    opacity: 0.0;
  }

  60% {
    opacity: 0.0;
  }

  100% {
    opacity: 1;
  }
}
