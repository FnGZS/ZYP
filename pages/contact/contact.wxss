.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-dianhua:before {
  content: "\e614";
}

.icon-liebiao:before {
  content: "\e602";
}

.icon-sousuo:before {
  content: "\e63c";
}
::-webkit-scrollbar{
  display: none
}
.top-search {
  display: flex;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 99999;
  height: 60rpx;
  background-color: #ef696c;
  vertical-align: middle;
  position: fixed;
}

.top-search .input-box {
  position: relative;
  flex: 6;
  width: 90%;
  margin-left: 5%;
  margin-right: 5%;
}

.top-search .input-box input {
  height: 40rpx;
  line-height: 40rpx;
  background-color: #fff;
  font-size: 25rpx;
  text-align: center;
  border-radius: 20rpx;
}

.choose {
  position: sticky;
  background: #fff;
  width: 100%;
  top: 60rpx;
  height: 80rpx;
  display: flex;
  line-height: 80rpx;
  border-bottom: 1rpx solid #f5f5f5;
  z-index: 999;
}

.icon-liebiao {
  margin-left: 5%;
  font-size: 32rpx;
}

.choose-title {
  font-size: 28rpx;
  line-height: 80rpx;
  margin-left: 10rpx;
}

.scroll-con {
  margin-top: 60rpx;
  background: #fff;
  border-left: 1rpx solid #f5f5f5;
}

.card-az {
  text-align: center;
  font-size: 25rpx;
}

.item-title {
  padding-left: 5%;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #f5f5f5;
  font-size: 25rpx;
  color: #8e8e93;
}

.card-li {
  position: relative;
  background-color: #fff;
  /* height: 100rpx; */
  padding: 17rpx 0;
  border-bottom: 1rpx solid #e5e5e5;
  display: flex;
}

.card-li .info-box {
  margin-left: 5%;
  width: 53%;
  color: #797979;
}

.info-box view {
  line-height: 50rpx;
  height: 50rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.info-box .big-name {
  margin-right: 20rpx;
  font-size: 25rpx;
  color: #4a4a4a;
}

.card-li .call-box {
  position: absolute;
  top: 0;
  right: 50rpx;
  height: 100%;
  width: 100rpx;
  text-align: center;
}

.info-phone {
  width: 32%;
  font-size: 28rpx;
  color: #4a4a4a;
}

.info-phoneList {
  display: flex;
  justify-content: flex-end;
}

.info-phone1 {
  line-height: 60rpx;
}

.info-phone2 {
  line-height: 60rpx;
}

.icon-dianhua {
  margin-left: 25rpx;
  line-height: 56rpx;
  color: #4a4a4a;
}

.side-az {
  position: fixed;
  /* top: 50rpx; */
  bottom: 0;
  right: 0rpx;
  text-align: center;
  width: 50rpx;
  z-index: 999;
}

.side-az .scroll-btn-group {
  display: flex;
  flex-direction: column;
}

.scroll-btn-group view {
  display: flex;
  align-items: center;
  flex: 1;
  padding-left: 15rpx;
}

.scroll-btn-group view text {
  color: #555;
  font-size: 24rpx;
}

.side-az image {
  margin-left: 10rpx;
  width: 20rpx;
  height: 20rpx;
}

.empty-search-view {
  position: relative;
  top: 0rpx;
  margin: 300rpx auto;
  text-align: center;
  font-size: 28rpx;
  color: #8d8d8d;
}

.empty-search-view image {
  width: 120rpx;
  height: 100rpx;
}

.page-right {
  position: fixed;
  width: 29.8%;
  height: 100%;
  z-index: 998;
  top: 0rpx;
  text-align: center;

}


.type {
  color: #585858;
  padding-top: 60rpx;
}

.typeList {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 28rpx;
  width: 100%;
}

.typeList-active {
  color: #fff;
  background: linear-gradient(to right, #ef696c, #f4a460);
}

.typeList-text {
  margin: 0rpx 15rpx;
}

.page-left {
  height: 100%;
  width: 100%;
  z-index: 999;
  position: absolute;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;

}

.c-state1 {
  transform: rotate(0deg) scale(1) translate(75%, 0%);
  -webkit-transform: rotate(0deg) scale(1) translate(75%, 0%);
}

.c-state2 {
  transform: rotate(0deg) scale(0.8) translate(75%, 0%);
  -webkit-transform: rotate(0deg) scale(0.8) translate(75%, 0%);
}
